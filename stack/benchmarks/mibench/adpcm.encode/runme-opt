#!/bin/sh
WORK=$HOME
SIMDIR=$WORK/Applications/gem5
$SIMDIR/build/X86/gem5.debug -q -r -e -d m5out/smmssm $SIMDIR/configs/example/se.py --cpu-type=atomic -c ssm_adpcm.encode --input="data/small.pcm" --output="ssm_output_small.adpcm"
$SIMDIR/build/X86/gem5.debug -q -r -e -d m5out/smmesm $SIMDIR/configs/example/se.py --cpu-type=atomic -c esm_adpcm.encode --input="data/small.pcm" --output="esm_output_small.adpcm"
