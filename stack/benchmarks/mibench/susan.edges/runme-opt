#!/bin/sh
WORK=$HOME
SIMDIR=$WORK/Applications/gem5
$SIMDIR/build/X86/gem5.debug -q -r -e -d m5out/smmssm $SIMDIR/configs/example/se.py --cpu-type=atomic -c ssm_susan.edges -o "input_small.pgm ssm_output_small.smoothing.pgm -e"
$SIMDIR/build/X86/gem5.debug -q -r -e -d m5out/smmesm $SIMDIR/configs/example/se.py --cpu-type=atomic -c esm_susan.edges -o "input_small.pgm esm_output_small.smoothing.pgm -e"
